<% form_for(@user) do |f| %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :password %><br />
    <%= f.text_field :password %>
  </p>


  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit 'Register' %>
  </p>
<% end %>

<%= link_to 'Back', stations_path %>